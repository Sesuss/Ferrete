<div class="container-fluid p-1">
  <div class="row mx-auto">
    <div class="col-md-12">
      <div class="row">

        <div class="col-md-4">
            <center>
          <img alt="" src="log.jpeg" width="700px" height="150px">
          <p>{{momentC}}</p>
          </center>
        </div>
        <div class="row" style="margin-top: 20px; margin-left:2px;" >
         <table class=" tabla-borderless " style="width: 100%; height: auto;" >
                                        <thead>
                                            <tr style="border-top: 1px solid black;">
                                            <th>Id</th>
                                            <th>Proveedor</th>
                                            <th>Categoria</th>
                                            <th>Descripcion</th>
                                            <th >Precio Venta</th>
                                            <th >Precio Compra</th>
                                            <th>Existencias</th>
                                            <th >Stock Minimo</th>
                                            </tr>
                                        </thead>
                                        <tbody >
                                            {{#each productos}}
                                            <tr style="border-top: 1px solid black;">
                                            <td>{{IdProducto}}</td>
                                            <td>{{Nombre}}</td>
                                            <td>{{Categoria}}</td>
                                            <td>{{Descripcion}}</td>
                                            <td>{{PrecioVenta}}</td>
                                            <td>{{PrecioCompra}}</td>
                                            <td>{{Existencias}}</td>
                                            <td>{{StockMinimo}}</td>
                                            </tr>
                                            {{/each}}
                                            </tbody>
                                        </table>
</div>
</div>
</div>
</div>
</div>